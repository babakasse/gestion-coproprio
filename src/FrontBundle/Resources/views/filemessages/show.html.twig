{%  extends "FrontBundle::base.html.twig" %}

{% block body %}
    <h1>Filemessage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fileMessage.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ fileMessage.titre }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{% if fileMessage.etat %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{% if fileMessage.dateDebut %}{{ fileMessage.dateDebut|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{% if fileMessage.dateFin %}{{ fileMessage.dateFin|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Iduser</th>
                <td>{{ fileMessage.idUser }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('filemessages_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('filemessages_edit', { 'id': fileMessage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
